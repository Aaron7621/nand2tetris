/**
 * 16-bit bitwise IsNg: 
 * if (in[15] == 1) then 1 else 0
 */

CHIP IsNg16 {
    IN in[16];
    OUT out;

    PARTS:
    // Put your code here:
    And(a=true, b=in[15], out=out);
}